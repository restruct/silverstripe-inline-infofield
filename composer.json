{
  "name": "restruct/silverstripe-inline-infofield",
  "description": "Drop-in configurable notice about cookies",
  "type": "silverstripe-vendormodule",
  "homepage": "http://github.com/restruct/silverstripe-inline-infofield",
  "keywords": [
    "silverstripe",
    "cookies"
  ],
  "authors": [ { "name": "Restruct", "email": "dev@restruct.nl", "homepage": "https://restruct.nl" } ],
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "^4.4"
  },
  "extra": {
    "expose": [
      "client/dist/js",
      "client/dist/css",
      "client/images"
    ],
    "installer-name": "inline-infofield"
  }
}
